{
  "name": "webpack-react-redux-es6-boilerplate",
  "version": "1.0.0",
  "description": "a webpack react redux es6 and boilerplate development environment",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local",
    "dev:local": "NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "NODE_ENV=production node build/webpack.release.js --uglify --deploy=online"
  },
  "author": "chacha",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react",
    "es6",
    "babel",
    "redux",
    "boilerplate"
  ],
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ftp": "^0.3.10",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "md5": "^2.1.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.10.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
