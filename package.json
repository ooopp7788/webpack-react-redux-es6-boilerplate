  "description": "a webpack, react, redux, es6, and boilerplate development environment",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "build": "export NODE_ENV=production && webpack --config webpack.production.js --progress --display-modules && npm run clean && mv _dist dist && npm run compress",
    "prod": "export NODE_ENV=production && webpack --config webpack.production.js --progress && npm run clean && mv _dist dist",
    "clean": "rimraf dist*",
    "compress": "zip -r dist.zip dist"
  },
